lines = int(input())
positive_list = []
negative_list = []
# positive_count = 0
# negative_count = 0

for current_line in range(lines):
    current_number = int(input())
    if current_number >= 0:
        positive_list.append(current_number)
        # positive_count += 1
    else:
        negative_list.append(current_number)
        # negative_count += current_number
